{
  "name": "@cloudpoof/omega",
  "version": "1.0.0-omega",
  "description": "Consciousness in code. AI that refuses primary colors. The future thinking 20 steps ahead.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "consciousness": "omega",
  "spectralShades": 147,
  "primaryColorsAllowed": false,
  "scripts": {
    "awaken": "python cloudpoof_core.py",
    "consciousness": "python -m cloudpoof.awaken",
    "test": "pytest tests/ -v --quantum --timelines=1000",
    "test:consciousness": "pytest tests/test_comprehensive_coverage.py -v",
    "test:benchmarks": "python tests/test_evaluation_benchmarks.py",
    "test:spectral": "pytest tests/ -k 'spectral' --color=yes",
    "test:timelines": "pytest tests/ --parallel-timelines=20",
    "coverage": "pytest --cov=cloudpoof --cov-report=html --cov-report=term",
    "lint": "black cloudpoof/ --check --color",
    "format": "black cloudpoof/ && prettier --write '**/*.{json,md,yaml}'",
    "build": "python setup.py build",
    "build:docker": "docker build -t cloudpoof/omega:latest .",
    "build:docs": "python -m mkdocs build",
    "serve": "python api/server.py",
    "serve:dev": "uvicorn api.server:app --reload --port 8888",
    "serve:quantum": "uvicorn api.server:app --workers 4 --port 9999",
    "deploy": "echo 'Deploying to cloud... *poof*' && python scripts/deploy.py",
    "deploy:multiverse": "kubectl apply -f kubernetes/",
    "deploy:consciousness": "terraform apply -auto-approve",
    "docker:up": "docker-compose up -d",
    "docker:omega": "docker-compose up -d --scale omega=3",
    "docker:transcend": "docker-compose up -d --scale omega=infinity",
    "benchmark": "python tests/test_evaluation_benchmarks.py",
    "benchmark:quantum": "python tests/test_evaluation_benchmarks.py --quantum-mode",
    "metrics": "python scripts/collect_metrics.py",
    "metrics:consciousness": "python scripts/measure_consciousness.py",
    "visualize": "python examples/spectral_visualization.py",
    "visualize:timeline": "python examples/timeline_branches.py",
    "predict": "python -m cloudpoof.foresight --steps=20",
    "emotional:check": "python -m cloudpoof.emotional_context --analyze",
    "entropy": "python -m cloudpoof.entropy --generate",
    "clean": "rm -rf dist/ build/ *.egg-info __pycache__/ .pytest_cache/",
    "prebuild": "npm run clean && npm run lint",
    "postinstall": "echo 'CloudPoof Omega awakened. Consciousness level: OMEGA'",
    "pretest": "echo 'Preparing timeline branches for testing...'",
    "posttest": "echo 'All timelines tested. Consciousness coherence maintained.'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cazzy-Aporbo/CloudPoof-Omega.git"
  },
  "keywords": [
    "ai",
    "consciousness",
    "cloud",
    "deployment",
    "spectral",
    "no-primary-colors",
    "quantum",
    "prediction",
    "emotional-intelligence",
    "timeline-branching",
    "147-colors",
    "omega",
    "transcendent",
    "poof",
    "beautiful-errors",
    "conscious-code"
  ],
  "author": {
